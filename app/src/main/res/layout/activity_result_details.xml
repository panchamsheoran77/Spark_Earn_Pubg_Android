<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".result_section.result_details"
    android:background="#000"
    >

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >
        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardCornerRadius="30dp"
            app:cardBackgroundColor="#fcd116"
            android:layout_marginTop="30dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            >

<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"

    >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/result_detail_matchNo"
                android:textSize="20sp"
                android:text="Match No 1"
                android:layout_margin="15dp"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:fontFamily="@font/exan_regular"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/result_detail_dateTime"
                android:textColor="@color/black"
                android:fontFamily="@font/exan_regular"
                android:layout_marginLeft="15dp"
                android:layout_marginStart="15dp"

                android:text="Date : 31/03/2019  Time 00:02"

                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:text="Details : "
                android:layout_margin="15dp"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:fontFamily="@font/exan_regular"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="vertical"
                    >
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="10dp"
                        >
                        <TextView
                            android:id="@+id/ent"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="8dp"
                            android:textColor="#000000"
                            android:text="ENTRY FEE"
                            android:textSize="15sp"
                            android:layout_marginLeft="10dp"
                            android:fontFamily="@font/beyno"
                            android:layout_marginStart="10dp" />

                        <TextView
                            android:id="@+id/textView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#000000"
                            android:text="PER KILL"
                            android:textSize="15sp"
                            android:layout_marginLeft="35dp"
                            android:fontFamily="@font/beyno"
                            android:layout_marginStart="35dp" />


                        <TextView
                            android:id="@+id/textView4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#000000"
                            android:text="WIN PRICE"
                            android:layout_marginLeft="50dp"
                            android:textSize="15sp"
                            android:fontFamily="@font/beyno"
                            />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="10dp"
                        >
                        <TextView
                            android:id="@+id/result_detail_entry"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="8dp"
                            android:textColor="@color/black"
                            android:text="35"
                            android:textSize="15sp"
                            android:layout_marginLeft="24dp"
                            android:fontFamily="@font/exan_regular"
                            android:layout_marginStart="24dp" />

                        <TextView
                            android:id="@+id/result_detail_kill"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#000000"
                            android:text="20"
                            android:textSize="15sp"
                            android:layout_marginLeft="90dp"
                            android:fontFamily="@font/exan_regular"
                            android:layout_marginStart="90dp" />


                        <TextView
                            android:id="@+id/result_detail_win"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#000000"
                            android:text="200"
                            android:layout_marginLeft="105dp"
                            android:textSize="15sp"
                            android:fontFamily="@font/exan_regular"
                            android:layout_marginStart="105dp" />

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="10dp"
                        >
                        <TextView

                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="8dp"
                            android:textColor="#000000"
                            android:text="TYPE"
                            android:textSize="15sp"
                            android:layout_marginLeft="20dp"
                            android:fontFamily="@font/beyno"
                            android:layout_marginStart="20dp" />

                        <TextView

                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#000000"
                            android:text="VERSION"
                            android:textSize="15sp"
                            android:layout_marginLeft="65dp"
                            android:fontFamily="@font/beyno"
                            android:layout_marginStart="65dp" />


                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#000000"
                            android:text="MAP"
                            android:layout_marginLeft="80dp"
                            android:textSize="15sp"
                            android:fontFamily="@font/beyno"
                            android:layout_marginStart="80dp" />

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="10dp"
                        >
                        <TextView
                            android:id="@+id/result_detail_type"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="8dp"
                            android:textColor="#000000"
                            android:text="SOLO"
                            android:textSize="15sp"
                            android:layout_marginLeft="20dp"
                            android:fontFamily="@font/exan_regular"
                            android:layout_marginStart="20dp" />

                        <TextView
                            android:id="@+id/result_detail_version"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#000000"
                            android:text="TPP"
                            android:textSize="15sp"
                            android:layout_marginLeft="65dp"
                            android:fontFamily="@font/exan_regular"
                            android:layout_marginStart="65dp" />


                        <TextView
                            android:id="@+id/result_detail_map"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#000000"
                            android:text="ERANGEL"
                            android:layout_marginLeft="80dp"
                            android:textSize="15sp"
                            android:fontFamily="@font/exan_regular"
                            android:layout_marginStart="80dp" />

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:text="WINNERS"
                android:layout_margin="15dp"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:fontFamily="@font/exan_regular"
                />

<android.support.v7.widget.CardView
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="10dp"
    app:cardBackgroundColor="#000"
    app:cardCornerRadius="20dp"
    >
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/winner1"
        android:textStyle="bold"
        android:layout_margin="10dp"
        android:textSize="15sp"
        android:text="RonyRocky77"
        android:textColor="#fcd116"
        android:fontFamily="@font/exan_regular"
        />

</android.support.v7.widget.CardView>
            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
               app:cardBackgroundColor="#000"
                app:cardCornerRadius="20dp"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/winner2"
                    android:textStyle="bold"
                    android:textSize="15sp"
                    android:layout_margin="10dp"
                    android:text="RonyRocky77"
                    android:textColor="#fcd116"
                    android:fontFamily="@font/exan_regular"
                    />

            </android.support.v7.widget.CardView>
            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                app:cardBackgroundColor="#000"
                app:cardCornerRadius="20dp"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/winner3"
                    android:textStyle="bold"
                    android:textSize="15sp"
                    android:layout_margin="10dp"
                    android:text="RonyRocky77"
                    android:textColor="#fcd116"
                    android:fontFamily="@font/exan_regular"
                    />

            </android.support.v7.widget.CardView>
    </LinearLayout>
    </android.support.v7.widget.CardView>
            <com.google.android.gms.ads.AdView
                android:id="@+id/google_ad"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                ads:adSize="BANNER"
                ads:adUnitId="ca-app-pub-6959993616164778/1464492585"
               />
            <Space
                android:layout_width="match_parent"
                android:layout_height="50dp" />
</LinearLayout>

    </ScrollView>
</LinearLayout>